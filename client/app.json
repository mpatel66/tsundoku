{
  "expo": {
    "name": "Tsundoku",
    "slug": "tsundoku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-square.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-screens/default.png",
        "resizeMode": "cover", 
        "backgroundColor": "#fffbf8"
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.patel2984.meera.uk.tsundoku", 
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-logo.png",
        "backgroundColor": "#fffbf8"
      },
      "splash": {
        "ldpi": "./assets/splash-screens/LDPI_Splash.png",
        "mdpi": "./assets/splash-screens/MDPI_Splash.png",
        "hdpi": "./assets/splash-screens/HDPI_Splash.png",
        "xhdpi": "./assets/splash-screens/XHDPI_Splash.png",
        "xxhdpi": "./assets/splash-screens/XXHDPI_Splash.png",
        "xxxhdpi": "./assets/splash-screens/XXXHDPI_Splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#fffbf8"
      }
    },
    "web": {
      "favicon": "./assets/logo-square.png"
    }
  }
}
